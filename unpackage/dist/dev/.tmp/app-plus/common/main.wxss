
page {
	background-color: #fff;
	font-size: 28rpx;
	color: #333333;
	font-family: Helvetica Neue, Helvetica, sans-serif;
}
view,
scroll-view,
swiper,
button,
input,
textarea,
label,
navigator,
image {
	box-sizing: border-box;
}
@-webkit-keyframes rotate{
from{
		-webkit-transform: rotate(0);
		        transform: rotate(0);
}
to{
		-webkit-transform: rotate(360deg);
		        transform: rotate(360deg);
}
}
@keyframes rotate{
from{
		-webkit-transform: rotate(0);
		        transform: rotate(0);
}
to{
		-webkit-transform: rotate(360deg);
		        transform: rotate(360deg);
}
}

